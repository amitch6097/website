{
    "name": "website",
    "version": "1.0.0",
    "private": true,
    "dependencies": {
        "react": "^16.10.2",
        "react-dom": "^16.10.2",
        "react-router-dom": "^5.1.2",
        "react-spring": "^8.0.27"
    },
    "scripts": {
        "build": "cross-env NODE_PATH=./node_modules webpack --config webpack.dev.js",
        "watch": "cross-env NODE_PATH=./node_modules npm-run-all --parallel webpack:watch prettier:watch",
        "webpack:watch": "cross-env NODE_PATH=./node_modules webpack --config webpack.dev.js --watch",
        "start": "cross-env NODE_PATH=./node_modules HOT=true PORT=8080 npm-run-all --parallel webpack:start:hot prettier:watch",
        "webpack:start:hot": "cross-env NODE_PATH=./node_modules webpack-dev-server --hotOnly --config webpack.dev.js",
        "prettier:watch": "onchange \"**/*.js\" \"**/*.jsx\" \"**/*.ts\" \"**/*.tsx\" \"**/*.less\" \"**/*.json\" --exclude \"dist/**\" \"package-lock.json\" -- prettier --write {{changed}}",
        "release": "cross-env NODE_PATH=./node_modules MIN_SUFFIX=true webpack --config webpack.config.js",
        "release:backsupport": "cross-env NODE_PATH=./node_modules webpack --config webpack.config.js",
        "release:portable": "cross-env NODE_PATH=./node_modules MIN_SUFFIX=true PORTABLE=true webpack --config webpack.config.js",
        "test": "cross-env NODE_PATH=./node_modules jest",
        "test:watch:": "jest --watchAll",
        "test:watch-generate-output": "jest --watchAll --json --outputFile=.jest-test-results.json || true",
        "test:coverage": "jest --coverage",
        "storybook:pretty": "cross-env NODE_PATH=./node_modules npm-run-all --parallel storybook prettier:watch",
        "storybook": "cross-env NODE_PATH=./node_modules PORTABLE=true start-storybook -p 6006",
        "build-storybook": "cross-env NODE_PATH=./node_modules PORTABLE=true build-storybook -o ./dist/debug/storybook-static",
        "test:generate-output": "jest --json --outputFile=.jest-test-results.json || true",
        "buildAndRelease": "npm run clean && npm run test:coverage && npm-run-all --parallel release:backsupport release release:portable build && npm run build-storybook"
    },
    "devDependencies": {
        "@babel/core": "^7.6.4",
        "@babel/plugin-proposal-class-properties": "^7.5.5",
        "@babel/plugin-syntax-dynamic-import": "^7.2.0",
        "@babel/polyfill": "^7.6.0",
        "@babel/preset-env": "^7.6.3",
        "@babel/preset-react": "^7.6.3",
        "@storybook/addon-actions": "^5.2.4",
        "@storybook/addon-jest": "^5.2.4",
        "@storybook/addon-knobs": "^5.2.4",
        "@storybook/addon-links": "^5.2.4",
        "@storybook/addon-viewport": "^5.2.4",
        "@storybook/addons": "^5.2.4",
        "@storybook/react": "^5.2.4",
        "@types/classnames": "^2.2.9",
        "@types/enzyme": "^3.10.3",
        "@types/enzyme-adapter-react-16": "^1.0.5",
        "@types/jest": "^24.0.19",
        "@types/node": "^11.13.22",
        "@types/react": "^16.9.9",
        "@types/react-dom": "^16.9.2",
        "@types/storybook__react": "^4.0.2",
        "acorn": "^6.3.0",
        "awesome-typescript-loader": "^5.2.1",
        "babel-core": "^7.0.0-bridge.0",
        "babel-jest": "^23.6.0",
        "babel-loader": "^8.0.6",
        "copy-webpack-plugin": "^4.6.0",
        "cross-env": "^5.2.1",
        "css-loader": "^1.0.0",
        "enzyme": "^3.10.0",
        "enzyme-adapter-react-16": "^1.15.1",
        "enzyme-to-json": "^3.4.3",
        "file-loader": "^5.0.2",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^23.6.0",
        "less": "^3.10.3",
        "less-loader": "^4.1.0",
        "npm-run-all": "^4.1.5",
        "onchange": "^5.2.0",
        "prettier": "1.15.2",
        "react-docgen-typescript-webpack-plugin": "^1.1.0",
        "react-hot-loader": "^4.12.15",
        "react-test-renderer": "^16.10.2",
        "rimraf": "^2.7.1",
        "source-map-loader": "^0.2.4",
        "storybook-addon-smart-knobs": "^4.1.2",
        "storybook-readme": "^5.0.8",
        "style-loader": "^0.23.0",
        "ts-jest": "^23.10.5",
        "typescript": "^3.7.2",
        "webpack": "^4.41.2",
        "webpack-cli": "^3.3.9",
        "webpack-dev-server": "^3.8.2",
        "webpack-merge": "^4.2.2"
    },
    "browserslist": [
        ">0.2%",
        "not dead"
    ],
    "jest": {
        "roots": [
            "<rootDir>"
        ],
        "preset": "ts-jest",
        "modulePaths": [
            "../../Repository/code/React/components/",
            "../../Repository/code/React/"
        ],
        "coverageDirectory": "dist/debug/coverage",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json"
        ],
        "setupFiles": [
            "<rootDir>__tests__/setup/enzyme.js"
        ],
        "transform": {
            "\\.(ts|tsx)$": "ts-jest",
            "\\.(js|jsx)$": "babel-jest"
        },
        "testMatch": [
            "<rootDir>/__tests__/**/*.(ts|tsx|js|jsx)"
        ],
        "testPathIgnorePatterns": [
            "<rootDir>/__tests__/setup/",
            "<rootDir>/node_modules/"
        ],
        "moduleNameMapper": {
            "^.+\\.(css|less)$": "identity-obj-proxy",
            "PlatformComponents/(.*)": "<rootDir>/../../Repository/code/React/components/$1",
            "react-spring/renderprops": "<rootDir>/node_modules/react-spring/renderprops.cjs",
            "react-spring": "<rootDir>/node_modules/react-spring/web.cjs"
        }
    }
}
